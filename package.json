{
  "name": "jrnlwarp",
  "description": "manage developer journals with vim and git",
  "version": "1.1.1",
  "author": "Hekar Khani (@hekar)",
  "bin": {
    "jrnlwarp": "./bin/run"
  },
  "bugs": "https://github.com/hekar/jrnlwarp/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@types/fs-extra": "^9.0.1",
    "chai": "^4.2.0",
    "fs-extra": "^9.0.1",
    "moment": "^2.27.0",
    "simple-git": "^2.18.0",
    "tempy": "^0.6.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/mocha": "^5",
    "@types/node": "^14",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/hekar/jrnlwarp",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "jrnlwarp"
  },
  "repository": "hekar/jrnlwarp",
  "scripts": {
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
    "test:mocha": "mocha --forbid-only \"test/**/*.test.ts\"",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
